{
  "name": "shared",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./index.css": "./index.css",
    "./package.json": "./package.json"
  },
  "source": "./index.tsx",
  "main": "./index.tsx",
  "module": "./index.tsx",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "gen:build": "tsup ./index.tsx --format esm,cjs --dts --external react",
    "gen:dev": "tsup ./index.tsx --format esm,cjs --watch --dts --external react",
    "build": "pnpm run build:js",
    "build:js": "rollup -c ./rollup.config.mjs",
    "lint": "eslint \"**/*.ts*\"",
    "test": "jest --passWithNoTests",
    "generate:component": "turbo gen react-component",
    "design-token": "node ./src/designTokens/generator.mjs",
    "icon": "node ./src/icons/generator.mjs",
    "convert": "node ./src/icons/profiles/generator.mjs"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/plugin-transform-runtime": "^7.24.3",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-url": "^8.0.2",
    "@svgr/plugin-jsx": "^8.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/babel__plugin-transform-runtime": "^7.9.5",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.6",
    "@types/testing-library__jest-dom": "^5.14.8",
    "dotenv": "^16.3.1",
    "esbuild-plugin-svgr": "^2.0.0",
    "eslint": "^7.32.0",
    "eslint-config-custom": "workspace:*",
    "jest-config": "workspace:*",
    "node-fetch": "^3.3.2",
    "rollup": "^4.14.1",
    "rollup-plugin-node-externals": "^7.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "tsconfig": "workspace:*",
    "tslib": "^2.6.2",
    "tsup": "^5.10.1",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "react": "^18.0.2"
  },
  "dependencies": {
    "crypto": "^1.0.1"
  }
}
